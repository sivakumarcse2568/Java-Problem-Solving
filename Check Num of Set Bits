import java.util.*;
class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter a number to Check:");
		int n = sc.nextInt();
		int cnt=0;
		for(int i=0;i<32;i++) {
			if((n&1) != 0) {
				cnt++;
			}
			n = n>>1;
		}
		System.out.print(cnt);
	}
	

}
